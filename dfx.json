{
  "canisters": {
    "video_canister": {
      "build": "cargo build --target wasm32-unknown-unknown --package  video_canister --release",
      "candid": "src/video_backend/video_canister/video_canister.did",
      "wasm": "target/wasm32-unknown-unknown/release/video_canister.wasm",
      "type": "custom"
    },
    "profile_backend": {
      "build": "cargo build --target wasm32-unknown-unknown --package  profile_backend --release",
      "candid": "src/profile_backend/profile_backend.did",
      "wasm": "target/wasm32-unknown-unknown/release/profile_backend.wasm",
      "type": "custom"
    },
    "video_backend": {
      "dependencies": ["profile_backend"],
      "build": "cargo build --target wasm32-unknown-unknown --package  video_backend --release",
      "candid": "src/video_backend/video_backend.did",
      "wasm": "target/wasm32-unknown-unknown/release/video_backend.wasm",
      "type": "custom"
    },
    "ad_manager": {
      "dependencies": ["profile_backend", "video_backend"],
      "build": "cargo build --target wasm32-unknown-unknown --package  ad_manager --release",
      "candid": "src/ad_manager/ad_manager.did",
      "wasm": "target/wasm32-unknown-unknown/release/ad_manager.wasm",
      "type": "custom"
    },
    "video_assets": {
      "dependencies": ["video_backend", "ad_manager", "profile_backend"],
      "frontend": {
        "entrypoint": "src/video_assets/src/index.html"
      },
      "source": ["src/video_assets/src/assets", "dist/video_assets/"],
      "type": "assets"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1,
  "dfx": "0.8.1"
}

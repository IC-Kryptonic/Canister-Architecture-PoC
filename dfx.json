{
  "canisters": {
    "rust_video": {
      "build": "cargo build --target wasm32-unknown-unknown --package  rust_video --release",
      "candid": "src/rust_video/video.did",
      "wasm": "target/wasm32-unknown-unknown/release/rust_video.wasm",
      "type": "custom"
    },
    "video_assets": {
      "dependencies": ["rust_video"],
      "frontend": {
        "entrypoint": "src/video_assets/src/index.html"
      },
      "source": ["src/video_assets/src/assets", "dist/video_assets/"],
      "type": "assets"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1,
  "dfx": "0.7.1"
}

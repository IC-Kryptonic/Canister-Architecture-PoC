{
  "canisters": {
    "bucket": {
      "build": "cargo build --target wasm32-unknown-unknown --package  bucket --release",
      "candid": "src/video_dht/bucket/bucket.did",
      "wasm": "target/wasm32-unknown-unknown/release/bucket.wasm",
      "type": "custom"
    },
    "ad_canister": {
      "build": "cargo build --target wasm32-unknown-unknown --package  ad_canister --release",
      "candid": "src/ad_manager/ad_canister/ad_canister.did",
      "wasm": "target/wasm32-unknown-unknown/release/ad_canister.wasm",
      "type": "custom"
    },
    "ad_manager": {
      "build": "cargo build --target wasm32-unknown-unknown --package  ad_manager --release",
      "candid": "src/ad_manager/ad_manager.did",
      "wasm": "target/wasm32-unknown-unknown/release/ad_manager.wasm",
      "type": "custom"
    },
    "video_dht": {
      "build": "cargo build --target wasm32-unknown-unknown --package  video_dht --release",
      "candid": "src/video_dht/video_dht.did",
      "wasm": "target/wasm32-unknown-unknown/release/video_dht.wasm",
      "type": "custom"
    },
    "backend": {
      "dependencies": ["video_dht"],
      "build": "cargo build --target wasm32-unknown-unknown --package  backend --release",
      "candid": "src/backend/backend.did",
      "wasm": "target/wasm32-unknown-unknown/release/backend.wasm",
      "type": "custom"
    },
    "video_assets": {
      "dependencies": ["backend", "ad_manager"],
      "frontend": {
        "entrypoint": "src/video_assets/src/index.html"
      },
      "source": ["src/video_assets/src/assets", "dist/video_assets/"],
      "type": "assets"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1,
  "dfx": "0.8.0"
}

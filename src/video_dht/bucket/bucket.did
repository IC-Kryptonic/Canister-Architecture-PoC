type Video_Id = text;
type Chunk_Num = nat64;
type Chunk = blob;

service : {
    "createVideo": (Video_Id, Chunk_Num) -> () oneway;
    "insertChunk": (Video_Id, Chunk_Num, Chunk) -> () oneway;
    "getChunk": (Video_Id, Chunk_Num) -> (opt Chunk) query;
    "getHash": (Video_Id) -> (opt blob) query;
}

@startuml
activate Frontend
Frontend -> VideoCanister: get_info()
activate VideoCanister
Frontend <-- VideoCanister: get_info: video_info
deactivate VideoCanister

loop chunkNum times
    Frontend -> VideoCanister: get_chunk(#)
    activate VideoCanister
    Frontend <-- VideoCanister: get_chunk: chunk
    deactivate VideoCanister
end
deactivate Frontend
@enduml

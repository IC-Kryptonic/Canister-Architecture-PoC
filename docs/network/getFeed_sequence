@startuml
alt random
    Frontend -> VideoBackend: getRandomFeed()
    VideoBackend --> Frontend: getRandomFeed: feed
else user
    Frontend -> VideoBackend: getUserFeed(userPrincipal)
    VideoBackend -> ProfileBackend: getFeedInfo(userPrincipal)
    VideoBackend <-- ProfileBackend: getFeedInfo: feedInfo
    Frontend <-- VideoBackend: getUserFeed: feed
else creator_feed
    Frontend -> VideoBackend: getCreatorFeed(creatorPrincipal)
    VideoBackend --> Frontend: getCreatorFeed: feed
else owner_feed
    Frontend -> VideoBackend: getOwnerFeed(ownerPrincipal)
    VideoBackend --> Frontend: getOwnerFeed: feed
else search_feed
    Frontend -> VideoBackend: getSearchFeed(searchWord)
    VideoBackend --> Frontend: getSearchFeed: feed
end
@enduml
